{
    "name": "vektor/passwordless_auth",
    "description": "Passwordless authentication for Laravel using magic email links",
    "keywords": [
        "vektor",
        "laravel",
        "authentication",
        "passwordless",
        "magic-link"],
    "homepage": "https://github.com/vektor/passwordless_auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Dawes",
            "email": "david@vektor.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/auth": "^10.0|^11.0",
        "illuminate/mail": "^10.0|^11.0",
        "vektor/api": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vektor\\PasswordlessAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vektor\\PasswordlessAuth\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vektor\\PasswordlessAuth\\PasswordlessAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}