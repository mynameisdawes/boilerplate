{
    "name": "vektor/blog",
    "description": "Blog",
    "keywords": [
        "vektor",
        "blog"
    ],
    "homepage": "https://github.com/vektor/blog",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Dawes",
            "email": "david@vektor.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "datomatic/nova-markdown-tui": "^1.2",
        "laravel/nova": "^5.0",
        "spatie/laravel-sluggable": "^3.4",
        "whitecube/nova-flexible-content": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Vektor\\Blog\\": "src",
            "Vektor\\Blog\\Database\\Seeders\\": "database/seeders"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vektor\\Blog\\BlogServiceProvider"
            ]
        }
    }
}
